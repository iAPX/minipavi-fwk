{
  "name": "iapx/minipavi-fwk",
  "description": "PHP 8.1+ framework to support minipavi for Minitel and emulators.",
  "version": "0.8.2",
  "keywords": ["minitel"],
  "authors": [
    {
      "name": "Philippe iAPX Vigier",
      "email": "pvigier@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MiniPaviFwk\\": "src/",
      "MiniPavi\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "ludosevilla/minipavicli",
        "version": "1.0.4",
        "source": {
          "url": "https://github.com/ludosevilla/minipaviCli.git",
          "type": "git",
          "reference": "1.0.4"
        },
        "autoload": {
          "files": ["MiniPaviCli.php"]
        }
      }
    }
  ],
  "require": {
    "ludosevilla/minipavicli": "1.0.4",
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  }
}
